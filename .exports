export EDITOR="subl -w"
export CLICOLOR=1
export LSCOLORS=gxfxcxdxbxegedabagacad
export DOTFILES_DIR="$HOME/.dotfiles"
export EXTRA_DIR="$HOME/.extra"
export NVM_DIR="$HOME/.nvm"

# Tell grep to highlight matches
export GREP_OPTIONS='â€”color=auto'

export JAVA_HOME="/Library/Java/JavaVirtualMachines/jdk1.7.0_80.jdk/Contents/Home"
export PATH="/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/sbin:/usr/local/go/bin:$DOTFILES_DIR/bin"
export PATH="/usr/local/bin:/Library/PostgreSQL/9.5/bin:/Library/mongodb/bin:/Library/apache-maven-3.3.9/bin:$PATH"
export PATH="$HOME/workSpace/scripts/:$PATH"
export PATH="$HOME/workSpace/go/bin/:$PATH"
export PATH="$HOME/workSpace/Zebra/arcanist/arcanist/bin/:$PATH"
export GOPATH="$HOME/workSpace/go"

export WORKON_HOME=$HOME/.virtualenvs
export PROJECT_HOME=$HOME/Devel

# Prefer US English and use UTF-8.
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# Make Python use UTF-8 encoding for output to stdin, stdout, and stderr.
export PYTHONIOENCODING='UTF-8';

# Omit duplicates and commands that begin with a space from history.
export HISTCONTROL='ignoreboth';


